            key-positions = <0 1>;
            bindings = <&kp ESC>;
        };

        combo-semicoron {
            bindings = <&kp SEMICOLON>;
            key-positions = <28 29>;
        };

        combo-minus {
            bindings = <&kp MINUS>;
            key-positions = <27 28>;
        };
    };

    macros {
        gif: gif {
            label = "giphy";
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <>;
